{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "nethvoice defaults",
  "$id": "http://nethserver.org/json-schema/task/output/nethvoice/get-defaults",
  "description": "Get nethvoice default configuration values",
  "examples": [
    {
      "accepted_timezone_list": [
        "Europe/Amsterdam",
        "Europe/Berlin",
        "Europe/Brussels",
        "Europe/London"
      ],
      "proxy_status": {
        "proxy_installed": true,
        "module_id": "nethvoice-proxy1"
      }
    },
    {
      "accepted_timezone_list": [
        "Europe/Amsterdam",
        "Europe/Berlin"
      ],
      "proxy_status": {
        "proxy_installed": false
      }
    }
  ],
  "type": "object",
  "additionalProperties": false,
  "required": ["accepted_timezone_list", "proxy_status"],
  "properties": {
    "accepted_timezone_list": {
      "description": "List of accepted timezones",
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "proxy_status": {
      "description": "Status of the NethVoice proxy",
      "type": "object",
      "required": ["proxy_installed"],
      "additionalProperties": false,
      "properties": {
        "proxy_installed": {
          "description": "True if the NethVoice proxy is installed, false otherwise",
          "type": "boolean"
        },
        "module_id": {
          "description": "The module ID of the NethVoice proxy",
          "type": "string"
        }
      }
    }
  }
}
